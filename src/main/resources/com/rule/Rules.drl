package com.sz.expert.disease

import com.sz.expert.disease.Disease

rule "Tuberculosis"
	when 
		disease: Disease(
		    highFever
		    && violetLimbs
		    && cough
		    && bronchospasm
            && xray == "dark"
            && whistle
            && highWhiteBloodCells
		);
	then
		disease.setTuberculosis(true);
	end

