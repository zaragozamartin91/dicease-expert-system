package com.sz.expert.dicease

import com.sz.expert.dicease.Dicease

rule "Tuberculosis"
	when 
		dicease: Dicease(
		    highFever
		    && violetLimbs
		    && cough
		    && bronchospasm
            && xray == "dark"
            && whistle
            && highWhiteBloodCells
		);
	then
		dicease.setTuberculosis(true);
	end

